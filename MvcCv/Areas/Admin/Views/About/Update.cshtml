@model UpdateAboutDto
@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h1>Update About</h1>
<form method="post">
    <input type="hidden" asp-for="Id" />
    <label>Name</label>
    <input type="text" asp-for="Name" class="form-control"/>
    <br />
    <label>Surname</label>
    <input type="text" asp-for="Surname" class="form-control"/>
    <br />
    <label>Address</label>
    <input type="text" asp-for="Address" class="form-control" />
    <br />
    <label>Phone</label>
    <input type="text" asp-for="Phone" class="form-control" />
    <br />
    <label>Mail</label>
    <input type="text" asp-for="Mail" class="form-control" />
    <br />
    <label>Description</label>
    <textarea  asp-for="Description" class="form-control" rows="5"></textarea>
    <br />
    <label>LinkedIn Url</label>
    <input type="text" asp-for="LinkedInUrl" class="form-control" />
    <br />
    <label>Github Url</label>
    <input type="text" asp-for="GithubUrl" class="form-control" />
    <br />
    <button class="btn btn-outline-primary">Update</button>
</form>

