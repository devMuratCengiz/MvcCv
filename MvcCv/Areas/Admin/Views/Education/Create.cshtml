@model CreateEducationDto
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Create Education</h1>
<form method="post">
    <label>School Name</label>
    <input type="text" asp-for="SchoolName" class="form-control" />
    <br />
    <label>Department</label>
    <input type="text" asp-for="Department" class="form-control" />
    <br />
    <label>GPA</label>
    <input type="text" asp-for="GPA" class="form-control" />
    <br />
    <label>Start Date</label>
    <input type="text" asp-for="StartDate" class="form-control" />
    <br />
    <label>End Date</label>
    <input type="text" asp-for="EndDate" class="form-control" />
    <br />
    
    <button class="btn btn-primary">Save</button>
</form>

